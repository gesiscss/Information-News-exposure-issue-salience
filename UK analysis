    "id2word = corpora.Dictionary(texts)\n",
    "# Filter out words that occur less than 30 documents, or more than 50% of the documents.\n",
    "id2word.filter_extremes(no_below=1, no_above=0.4)\n",
    "\n",
    "# Create Corpus\n",
    "texts = texts"
   ]
