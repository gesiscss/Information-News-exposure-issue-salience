    "## Delete handles, image caption/credits, links, money amounts, phone numbers, and navigation menus"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "r_ignore_case = {\n",
    "    'handle': r'\\S+@\\S+',\n",
    "    'link': r'((visit|at)\\s+)*(\\S+).(com|org)((/|\\S)*)',\n",
    "    'money': r'\\$\\d*',\n",
    "    'phone': r'(\\d-)*(\\d+-\\d+-\\d+)' ,\n",
    "    'menu': r'(?<=•)[^•]*(?=•)'\n",
    "}\n",
    "r_set_case = {\n",
    "    'photographer-credits': r'PHOTO(GRAPHER)*:(\\s)*(([A-Z][A-Za-z]*)\\s*){1,3}',\n",
    "    'graphics': r'Graphic([^.])'\n",
    "}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "def filter_text(text):\n",
    "    for regex in r_ignore_case:\n",
    "        text = re.sub(regex, '', text, re.I)\n",
    "    for regex in r_set_case:\n",
    "        text = re.sub(regex, '', text)\n",
    "    return text"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "stemmer = PorterStemmer()\n",
    "def lemmatize_stemming(text):\n",
    "    return [stemmer.stem(WordNetLemmatizer().lemmatize(word, pos='v')) for word in text]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "def process_text(text):\n",
    "    text = re.sub('[^a-zA-Z]', ' ', text)\n",
    "    text = text.lower()\n",
    "    text = text.split()\n",
    "    return text"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "url = \"https://countwordsfree.com/stopwords/english/json\"\n",
    "response = pd.DataFrame(data = json.loads(requests.get(url).text))\n",
    "SW = list(response['words'])\n",
    "def remove_stopwords(text):\n",
    "    return [word for word in text if not word in SW]"
   ]
  },
